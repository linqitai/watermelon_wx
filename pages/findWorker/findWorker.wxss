.contentBox{
  padding: 0 30rpx;
  background-color: white;
}
.contentBox2{
  padding: 20rpx 30rpx;
  background-color: white;
  margin-top: 10rpx;
}
/* ====================== */
.inputBox{
  font-size: 34rpx;
  display: flex;
  height: 86rpx;
  align-items: center;
  border-bottom: 1rpx solid #EEEEEE;
  overflow: hidden;
}
.inputBox:last-child{
  border-bottom-color: transparent !important;
}
.workerType{
  border-radius: 20rpx;
  background-color: #DFEFED;
  margin-top: 20rpx;
  position: relative;
  overflow: hidden;
  padding-bottom: 20rpx;
}
.workerTypeBlock{
  padding: 4rpx 16rpx;
  background-color: white;
  margin-top: 20rpx;
  margin-left: 20rpx;
  float: left;
  border-radius: 6rpx;
  color: #048176;
  font-size: 32rpx;
  border: 1px solid transparent;
}
.blockBorder{
  /* border-color: #048176; */
  background-color: #048176;
  color: white;
}
.addBorder{
  border-color: #003b36;
  box-shadow: 0px 0px 1px 2px #048176;
  /* color: white; */
}
.label{
  flex: 0 0 200rpx;
  color: #919191;
}
.label2{
  flex: 0 0 70rpx;
  padding-left: 20rpx;
}
.checkedIcon{
  color: #048176;
  /* color: #c7c7c7; */
}
.text2{
  color: #919191;
  font-size: 32rpx;
}
.input{
  flex: auto;
}
.picker{
  flex: auto;
}
.select{
  width: 100%;
}
.iconArrow{
  float: right;
  margin-top: 12rpx;
  margin-right: 20rpx;
  color: #c7c7c7;
  font-size: 32rpx;
}
.inputP{
  color: #c7c7c7;
}
/* ======================== */
.inputBoxTextarea{
  font-size: 34rpx;
  display: flex;
  align-items: center;
  padding: 22rpx 0;
  border-bottom: 1rpx solid #EEEEEE;
}
.labelTextarea{
  flex: 0 0 200rpx;
  color: #919191;
}
.inputTextarea{
  flex: auto;
  z-index: 20;
}
.selecMoreHeader{
  display: block;
  font-size: 38rpx;
  padding: 20rpx 0;
}
.dialogBlockBox{
  display: block;
  background-color: #DFEFED;
  padding-bottom: 20rpx;
  position: relative;
  overflow: hidden;
}
.selectMoreDialogSureBtn{
    display: block;
    color: #FFB701;
    font-size: 44rpx;
    width: 90%;
    text-align: center;
    height: 100rpx;
    line-height: 100rpx;
    border-top: 1rpx solid #c7c7c7;
    margin-top: 20rpx;
    margin-left: 50%;
    transform: translateX(-50%);
    -o-transform: translateX(-50%);
    -moz-transform: translateX(-50%);
}
.dialogSure{
  z-index: 2000;
  width: 70%;
  background-color: white;
  padding: 0 30rpx;
  font-size: 32rpx;
  justify-content: space-around;
  color: #919191;
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%,-70%);
  -o-transform: translate(-50%,-70%);
  -moz-transform: translate(-50%,-70%);
  border-radius: 40rpx;
  /* animation:all 2s ease; */
}

/* @-webkit-keyframes mymove{
  from {bottom:0px;}
  to {bottom:200px;}
} */
.dialogSureBox{
  position: relative;
  padding-top: 100rpx;
  background-color: white;
}
.dialogTitle{
  display: block;
  text-align: center;
  font-size: 38rpx;
}
.dialogSureIcon{
  width: 288rpx;
  height: 178rpx;
  position: absolute;
  left: 50%;
  top: -80rpx;
  transform: translateX(-50%);
  -o-transform: translateX(-50%);
  -moz-transform: translateX(-50%);
}
.dialogText{
  line-height: 60rpx;
  color: #919191;
}
.dialogSureBtn{
  display: block;
  color: #FFB701;
  font-size: 44rpx;
  width: 90%;
  text-align: center;
  height: 100rpx;
  line-height: 100rpx;
  border-top: 1rpx solid #c7c7c7;
  margin-top: 20rpx;
  margin-left: 50%;
  transform: translateX(-50%);
  -o-transform: translateX(-50%);
  -moz-transform: translateX(-50%);
}
.dialogSureBtn:active{
  font-size: 40rpx;
}
/* ================ */
.mask{
  position: fixed;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  background-color: rgba(0, 0, 0, 0.5);
  z-index: 999;
}